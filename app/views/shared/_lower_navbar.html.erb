<% unless current_page?(root_path) || current_page?(new_user_session_path) || current_page?(new_user_registration_path) %>
  <div class="navbar-space"></div>
  <div class="bottom-fixed">
    <div class="lower-navbar">
      <div class="lower-navbar-strip"></div>
      <div class="lower-navbar-buttons">
        <div class="back-btn-container">
          <%= link_to :back, class: "navbar-back" do %>
            <i class="fas fa-angle-left"></i>
          <% end %>
        </div>
        <div class="poke-btn-container">
          <%= link_to users_path do %>
            <div class="poke-btn">
              <i class="fas fa-home"></i>
            </div>
          <% end %>
        </div>
        <%= link_to pokes_path, class: "navbar-messages" do %>
          <i class="fas fa-comment-alt"></i>
        <% end %>
      </div>
    </div>
  </div>
<% end %>
