<h1>Pokes#index</h1>
<p>Find me in app/views/pokes/index.html.erb</p>


<div class="poke">
  <% @my_sent_pokes.each do |poke|%>
      <img class="md-avatar"src="<%= image_path 'boy1.jpg' %>">
      <div class="poke-name">
        <h3><%= poke.receiver.first_name %></h3>
      </div>
      <div class="poke-body">
        <p><strong> Poked

          <%= poke.poke_created_time %>
           ago
        </strong></p>
      </div>
  <% end %>
  <% @my_received_pokes.each do |poke|%>
      <img class="avatar-large" src="">
      <div class="poke-name">
        <h3><%= poke.sender.first_name %></h3>
      </div>
      <div class="poke-body">
        <p><strong> Poked

          <%= poke.poke_created_time %>
           ago
        </strong></p>
      </div>
  <% end %>
</div>
